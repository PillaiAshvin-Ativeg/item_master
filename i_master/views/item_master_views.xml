<odoo>
    <record id="view_item_master_form" model="ir.ui.view">
        <field name="name">item.master.form</field>
        <field name="model">item.master</field>
        <field name="arch" type="xml">
            <form string="Item Master">
                    <group>
                        <field name="brand"/>
                        <field name="segment"/>
                        <field name="width"/>
                        <field name="length"/>
                        <field name="height"/>
                        <field name="noon"/>
                        <field name="amazon"/>
                        <field name="internal_id"/>
                    </group>
            </form>
        </field>
    </record>

    <record id="view_item_master_list" model="ir.ui.view">
        <field name="name">item.master.list</field>
        <field name="model">item.master</field>
        <field name="arch" type="xml">
            <list>
                <field name="brand"/>
                <field name="segment"/>
                <field name="width"/>
                <field name="length"/>
                <field name="height"/>
                <field name="noon"/>
                <field name="amazon"/>
                <field name="internal_id"/>
            </list>
        </field>
    </record>

    <record id="action_item_master" model="ir.actions.act_window">
        <field name="name">Item Master</field>
        <field name="res_model">item.master</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>